{
  "name": "angular-compiler-plugin",
  "version": "1.0.0",
  "description": "Angular2 Offiline Compiler and Code Generator Plugin",
  "main": "lib.babel/entry.js",
  "scripts": {
    "ts": "./node_modules/.bin/tsc --project src --watch  --pretty --diagnostics",
    "ts-build": "./node_modules/.bin/tsc --project src --pretty",
    "babel": "babel lib --presets es2015 --out-dir lib.babel --watch",
    "babel-build": "babel lib --presets es2015 --out-dir lib.babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheLarkInn/AngularCompilerPlugin.git"
  },
  "keywords": [
    "Angular2",
    "Angular",
    "Compiler",
    "Webpack",
    "Plugin",
    "Offline",
    "Code",
    "Generation",
    "Codegen",
    "CompilerHost"
  ],
  "author": "Sean Larkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheLarkInn/AngularCompilerPlugin/issues"
  },
  "homepage": "https://github.com/TheLarkInn/AngularCompilerPlugin#readme",
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-es2015-node4": "^1.0.0",
    "babel-preset-stage-2": "^6.1.2",
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/compiler-cli": "^0.2.0",
    "@angular/core": "^2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-server": "^2.0.0-rc.1",
    "bluebird": "^3.3.5",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "rxjs": "^5.0.0-beta.7",
    "tslint": "^3.10.1",
    "typescript": "^1.9.0-dev.20160428-1.0",
    "webpack": "^1.13.0",
    "zone.js": "^0.6.12",
    "awesome-typescript-loader": "^0.17.0",
    "colors": "^1.1.2",
    "parse-json": "^2.2.0",
    "strip-bom": "^3.0.0",
    "strip-json-comments": "^2.0.1",
    "tsconfig": "^2.2.0"
  }
}
